{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "rules": {
    "strict": 0,
    "no-alert": 1,
    "no-eval": 2,
    "semi": [2, "never"],
    "quotes": [2, "single"],
    "no-use-before-define": 2,
    "no-unused-vars": 2
  }
}